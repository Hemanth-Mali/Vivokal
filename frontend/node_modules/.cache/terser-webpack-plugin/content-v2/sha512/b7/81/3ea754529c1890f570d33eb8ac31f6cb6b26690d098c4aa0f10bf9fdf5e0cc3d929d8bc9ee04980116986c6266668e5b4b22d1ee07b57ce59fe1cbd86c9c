port small\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"p\",{children:\"When you buy from a small business,an actual person does a little happy dance.\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"h5\",{children:\"#VIZAGVOCALFORLOCAL\"})]})}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(Col[\"a\" /* default */],{lg:4,md:6,sm:12,children:/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(Image[\"a\" /* default */],{src:\"/images/homeheart.png\",style:{width:'155px',padding:'7%'}}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"h4\",{children:\"For the Love of Local\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"p\",{children:\"Love local and Live local\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"h5\",{children:\"#SHOPLOCAL\"})]})})]})]}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"br\",{}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"br\",{}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"br\",{}),/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"section\",{style:{textAlign:'center',backgroundColor:'black'},children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"br\",{}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"br\",{}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"h1\",{style:{color:'#fff'},children:\"Our Users Opinions\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"br\",{}),/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(Carousel[\"a\" /* default */],{style:{padding:'5%'},children:[/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(Carousel[\"a\" /* default */].Item,{as:\"div\",style:{padding:'5%'},children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"h3\",{style:{color:'#fff'},children:\"\\u201CGoing local does not mean walling off the outside world. It Means nurturing locally owned businesses which use local resources sustainably and serve primarily local consumers.\\u201D\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(Image[\"a\" /* default */],{roundedCircle:true,style:{width:'150px'},src:\"/images/hemanth.jpeg\",alt:\"Hemanth\